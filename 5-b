#include <stdio.h>

int main() {
    int arr[100], n, i, first;

    printf("Enter number of elements in array: ");
    scanf("%d", &n);

    printf("Enter %d elements: ", n);
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    // Store first element before rotation
    first = arr[0];

    // Shift all elements one position to the left
    for (i = 0; i < n - 1; i++) {
        arr[i] = arr[i + 1];
    }

    // Put first element at the end
    arr[n - 1] = first;

    printf("\nArray after left rotation by one position:\n");
    for (i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }

    printf("\n");
    return 0;
}
